<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- left leg -->
  <xacro:macro name="rhphumanoid_l_leg" params="parent">

  <!-- left hip yaw joint -->
  <joint name="l_hip_yaw" type="revolute">
    <parent link="${parent}"/>
    <child link="l_hip_yaw_link"/>
    <origin xyz="0.017000 0.034930 -0.045670" rpy="0 0 0" />
    <axis xyz="0 0 -1" />
    <limit effort="${45*9.80665/100}" velocity="${(pi/3)/0.18}" lower="${-pi*2/3}" upper="${pi*2/3}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>
  <!-- <xacro:insert_transmission name="l_hip_yaw" /> -->

  <!-- left hip yaw link -->
  <link name="l_hip_yaw_link">
    <visual>
      <xacro:insert_mesh_l_hip_yaw_link />
    </visual>
    <collision>
      <xacro:insert_mesh_l_hip_yaw_link />
    </collision>
    <!-- <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0 0 0"/>
      </geometry>
    </collision> -->
    <inertial>
      <xacro:insert_inertia_l_hip_yaw_link />
    </inertial>
  </link>

  <!-- left hip roll joint -->
  <joint name="l_hip_roll" type="revolute">
    <parent link="l_hip_yaw_link"/>
    <child link="l_hip_roll_link"/>
    <origin xyz="-0.015830 0.000000 -0.035400" rpy="0 0 0" />
    <axis xyz="-1 0 0" />
    <limit effort="${45*9.80665/100}" velocity="${(pi/3)/0.18}" lower="-2.0" upper="2.0" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>
  <!-- <xacro:insert_transmission name="l_hip_roll" /> -->

  <!-- left hip roll link -->
  <link name="l_hip_roll_link">
    <visual>
      <xacro:insert_mesh_l_hip_roll_link />
    </visual>
    <collision>
      <xacro:insert_mesh_l_hip_roll_link />
    </collision>
    <!-- <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0 0 0"/>
      </geometry>
    </collision> -->
    <inertial>
      <xacro:insert_inertia_l_hip_roll_link />
    </inertial>
  </link>

  <!-- left hip pitch joint -->
  <joint name="l_hip_pitch" type="revolute">
    <parent link="l_hip_roll_link"/>
    <child link="l_hip_pitch_link"/>
    <origin xyz="0.016330 0.025420 0.000000" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <limit effort="${45*9.80665/100}" velocity="${(pi/3)/0.18}" lower="-0.55" upper="${pi*2/3}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>
  <!-- <xacro:insert_transmission name="l_hip_pitch" /> -->

  <!-- left hip pitch link -->
  <link name="l_hip_pitch_link">
    <visual>
      <xacro:insert_mesh_l_hip_pitch_link />
    </visual>
    <collision>
      <xacro:insert_mesh_l_hip_pitch_link />
    </collision>
    <!-- <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0 0 0"/>
      </geometry>
    </collision> -->
    <inertial>
      <xacro:insert_inertia_l_hip_pitch_link />
    </inertial>
  </link>

  <!-- left knee joint -->
  <joint name="l_knee" type="revolute">
    <parent link="l_hip_pitch_link"/>
    <child link="l_knee_link"/>
    <origin xyz="0.000000 0.000000 -0.072100" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <limit effort="${45*9.80665/100}" velocity="${(pi/3)/0.18}" lower="${-pi*2/3}" upper="1.2" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>
  <!-- <xacro:insert_transmission name="l_knee" /> -->

  <!-- left knee link -->
  <link name="l_knee_link">
    <visual>
      <xacro:insert_mesh_l_knee_link />
    </visual>
    <collision>
      <xacro:insert_mesh_l_knee_link />
    </collision>
    <!-- <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0 0 0"/>
      </geometry>
    </collision> -->
    <inertial>
      <xacro:insert_inertia_l_knee_link />
    </inertial>
  </link>

  <!-- left ankle pitch joint -->
  <joint name="l_ank_pitch" type="revolute">
    <parent link="l_knee_link"/>
    <child link="l_ank_pitch_link"/>
    <origin xyz="0.000000 0.000000 -0.072100" rpy="0 0 0" />
    <axis xyz="0 -1 0" />
    <limit effort="${45*9.80665/100}" velocity="${(pi/3)/0.18}" lower="${-pi*2/3}" upper="${pi*2/3}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>
  <!-- <xacro:insert_transmission name="l_ank_pitch" /> -->

  <!-- left ankle pitch link -->
  <link name="l_ank_pitch_link">
    <visual>
      <xacro:insert_mesh_l_ank_pitch_link />
    </visual>
    <collision>
      <xacro:insert_mesh_l_ank_pitch_link />
    </collision>
    <!-- <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0 0 0"/>
      </geometry>
    </collision> -->
    <inertial>
      <xacro:insert_inertia_l_ank_pitch_link />
    </inertial>
  </link>

  <!-- left ankle roll joint -->
  <joint name="l_ank_roll" type="revolute">
    <parent link="l_ank_pitch_link"/>
    <child link="l_ank_roll_link"/>
    <origin xyz="-0.016330 -0.025420 0.000000" rpy="0 0 0" />
    <axis xyz="1 0 0" />
    <limit effort="${45*9.80665/100}" velocity="${(pi/3)/0.18}" lower="-1.9" upper="1.9" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>
  <!-- <xacro:insert_transmission name="l_ank_roll" /> -->

  <!-- left ankle roll link -->
  <link name="l_ank_roll_link">
    <visual>
      <xacro:insert_mesh_l_ank_roll_link />
    </visual>
    <collision>
      <xacro:insert_mesh_l_ank_roll_link />
    </collision>
    <!-- <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0 0 0"/>
      </geometry>
    </collision> -->
    <inertial>
      <xacro:insert_inertia_l_ank_roll_link />
    </inertial>
  </link>

  </xacro:macro>

</robot>
